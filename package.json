{
  "name": "mtbridge",
  "version": "3.0.7",
  "description": "Metatrader API Bridge",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dist:mac:arm64": "pkg index.js -t node14-macos-arm64 -o upload/mtbridge-server-arm64",
    "dist:mac:x64": "pkg index.js -t node14-macos-x64 -o upload/mtbridge-server-x64",
    "dist:linux:arm64": "pkg index.js -t node14-linux-arm64 -o upload/mtbridge-server-linux-arm64",
    "dist:linux:x64": "pkg index.js -t node14-linux-x64 -o upload/mtbridge-server-linux-x64",
    "dist:win:arm64": "pkg index.js -t node14-win-arm64 -o upload/mtbridge-server-arm64.exe",
    "dist:win:x64": "pkg index.js -t node14-win-x64 -o upload/mtbridge-server-x64.exe",
    "dist:clean": "rm upload/mtbridge-server*"
  },
  "keywords": [
    "metatrader",
    "forex",
    "presspage",
    "entertainment",
    "redeecash"
  ],
  "author": {
    "name": "Patrick Ingle",
    "company": "PressPage Entertainment Inc",
    "email": "presspage.entertainment@gmail.com",
    "url": "https://presspage.info"
  },
  "license": "CC-BY-4.0",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "killable": "^1.0.1",
    "nodejs-md5": "^1.1.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}
